<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leonbec.swoosh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Controller.WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Controller.LeagueActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Controller.BaseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Controller.SkillActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Controller.FinishActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>